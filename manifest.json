{
  "name": "Github JS/TS file navigation",
  "version": "1.0",
  "description": "Navigate through imported file in javascript files",
  "permissions": ["tabs", "https://*.github.com/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
